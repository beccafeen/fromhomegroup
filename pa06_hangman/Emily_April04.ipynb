    "--------\n",
    "\"\"\",\n",
    "\"\"\"\n",
    "-----\n",
    "|   |\n",
    "|   0\n",
    "| /   \\ \n",
    "|   |  \n",
    "|  | |\n",
    "|\n",
    "--------\n",
    "\"\"\")\n",
    "    want_to_play = True\n",
    "    play = input('Do you want to play hangman?'.lower())\n",
    "\n",
    "    if play not in (\"yes\",\"y\"):\n",
    "        want_to_play = False\n",
    "        print('Goodbye')\n",
    "        \n",
    "        \n",
    "    \n",
    "    while want_to_play == True:\n",
    "        print(HANGMAN[0])\n",
    "        word = generate_random_word()\n",
    "        print(word)\n",
