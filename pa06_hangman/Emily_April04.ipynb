    "                    print(\"That is not a letter. Please try again.\")\n",
    "                    \n",
    "                elif len(letter) > 1: # check the input is only one letter\n",
    "                    attempts -= 1\n",
    "                    print(HANGMAN[(len(HANGMAN) - 1) - attempts])\n",
    "                    print(\"That is more than one letter. Please try again.\")\n",
    "                    \n",
